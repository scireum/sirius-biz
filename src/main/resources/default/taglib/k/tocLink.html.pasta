<i:arg type="String" name="link" description="Defines a url or link."/>

<i:pragma name="description">
    Renders an entry for the table of content. Should be used within the k:toc tag.
</i:pragma>

<t:navboxLink url="@link" icon="fa-solid fa-chevron-right">
    <i:render name="body"/>
</t:navboxLink>
