<i:arg type="String" name="point"/>
<i:pragma name="priority" value="100"/>

<i:switch test="@point">
    <i:block name="help-navbox">
        <i:if test="isFrameworkEnabled('tycho.knowledge-base')">
            <t:navboxLink labelKey="KnowledgeBase.kb" url="/kb" icon="fa-solid fa-lightbulb"/>
        </i:if>
    </i:block>
</i:switch>
