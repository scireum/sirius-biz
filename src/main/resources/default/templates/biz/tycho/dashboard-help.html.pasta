<i:local name="contents" value="renderToString('body')" />
<i:local name="navbox" value="renderToString('navbox')" />
<i:if test="isFilled(contents) || isFilled(navbox)">
    <div class="card mb-4">
        <div class="card-body">
            <h5 class="card-title">
                @i18n("DashboardController.help")
            </h5>

            <i:raw>@contents</i:raw>

            <t:navbox>
                <i:raw>@navbox</i:raw>
            </t:navbox>
        </div>
    </div>
</i:if>
